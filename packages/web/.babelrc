{
  "plugins": [
    ["relay", { "artifactDirectory": "./src/__generated__" }],
    "macros",
    "transform-class-properties",
    "react-refresh/babel"
  ],
  "presets": [
    [
      "@babel/preset-env",
      { "useBuiltIns": "usage", "corejs": { "version": 3 } }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}
